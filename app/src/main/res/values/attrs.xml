<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Lrc">
        <attr name="textSize" format="dimension" />
        <attr name="normalTextColor" format="reference|color" />
        <attr name="currentTextColor" format="reference|color" />
        <attr name="dividerHeight" format="dimension" />
        <attr name="rows" format="integer" />
    </declare-styleable>

    <declare-styleable name="SlideMode">
        <attr name="mode">
            <enum name="forbid" value="0"></enum>
            <enum name="right" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>
